  "devDependencies": {
    "@esm-bundle/chai": "^4.3.4",
    "@snowpack/plugin-postcss": "^1.4.1",
    "@snowpack/web-test-runner-plugin": "^0.2.2",
    "@web/dev-server-esbuild": "^0.2.12",
    "@web/test-runner": "^0.12.20",
    "esbuild": "^0.8.57",
    "postcss": "^8.3.0",
